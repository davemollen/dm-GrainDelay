@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<https://github.com/davemollen/dm-GrainDelay>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-dm-graindelay.html> ;
        modgui:stylesheet <modgui/stylesheet-dm-graindelay.css> ;
        modgui:screenshot <modgui/screenshot-dm-graindelay.png> ;
        modgui:thumbnail <modgui/thumbnail-dm-graindelay.png> ;
        modgui:brand "dm" ;
        modgui:label "Grain Delay" ;
        modgui:model "boxy" ;
        modgui:panel "8-knobs" ;
        modgui:color "white" ;
        modgui:knob "silver" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "spray" ;
            lv2:name "Spray" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "frequency" ;
            lv2:name "Freq" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "pitch" ;
            lv2:name "Pitch" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "rand_pitch" ;
            lv2:name "Rand Pitch" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "delay_time" ;
            lv2:name "Time" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "feedback" ;
            lv2:name "Feedback" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "low_cut" ;
            lv2:name "Low Cut" ;
        ] , [
            lv2:index 7 ;
            lv2:symbol "mix" ;
            lv2:name "Mix" ;
        ] ;
    ] .
